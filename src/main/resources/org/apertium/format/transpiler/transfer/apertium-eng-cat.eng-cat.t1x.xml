    <section-rules>
    
        <!--********************************* REGLES DE SN ***************************************************** -->
    
        <!-- primer les regles lèxiques-->
        <rule comment="REGLA: MONTH NUM, NUM (July 23, 2010 → 23 de juliol de 2010)">
            <pattern>
                <pattern-item n="months"/>
                <pattern-item n="num"/>
                <pattern-item n="coma"/>
                <pattern-item n="num"/>
            </pattern>
            <action>
                <call-macro n="firstWord">
                    <with-param pos="1"/>
                </call-macro>
                <choose>
                    <when>
                        <test>
                            <equal>
                                <clip pos="1" side="sl" part="lem"/>
                                <lit v="March"/>
                            </equal>
                        </test>
                        <let>
                            <clip pos="1" side="tl" part="lem"/>
                            <lit v="març"/>
                        </let>
                        <choose>
                            <when>
                                <test>
                                    <equal>
                                        <clip pos="1" side="sl" part="a_nom"/>
                                        <lit-tag v="np.cog"/>
                                    </equal>
                                </test>
                                <let>
                                    <clip pos="1" side="tl" part="a_nom"/>
                                    <lit-tag v="n"/>
                                </let>
                                <let>
                                    <clip pos="1" side="tl" part="gen"/>
                                    <lit-tag v="m"/>
                                </let>
                                <let>
                                    <clip pos="1" side="tl" part="nbr"/>
                                    <lit-tag v="sg"/>
                                </let>
                            </when>
                        </choose>
                    </when>
                </choose>
                <out>
                    <chunk name="month_num_cm_num" case="caseFirstWord">
                        <tags>
                            <tag>
                                <clip pos="2" side="tl" part="a_adv"/>
                            </tag>
                            <tag>
                                <var n="neg"/>
                            </tag>
                        </tags>
                        <lu>
                            <clip pos="2" side="tl" part="whole"/>
                        </lu>
                        <b pos="1"/>
                        <lu>
                            <lit v="de"/>
                            <lit-tag v="pr"/>
                        </lu>
                        <b/>
                        <lu>
                            <clip pos="1" side="tl" part="whole"/>
                        </lu>
                        <b/>
                        <lu>
                            <lit v="de"/>
                            <lit-tag v="pr"/>
                        </lu>
                        <b pos="3"/>
                        <lu>
                            <clip pos="4" side="tl" part="whole"/>
                        </lu>
                    </chunk>
                </out>
            </action>
        </rule>
        
    </section-rules>
    
